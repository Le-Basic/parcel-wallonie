{
  "name": "parcel-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vitest --run && vue-cli-service serve",
    "build": "vitest --run && vue-cli-service build",
    "lint:format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,html}\" --config  \".prettierrc\" --ignore-path  \".eslintignore\"",
