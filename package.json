    "echarts": "^5.4.3",
    "jquery": "^3.7.1",
    "rangeslider.js": "^2.3.3",
    "topojson-client": "^3.1.0",
    "vue": "^3.2.13",
    "vue-echarts": "^6.6.8",
    "vue-plugin-load-script": "^2.1.1",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vitest": "^1.2.1"
